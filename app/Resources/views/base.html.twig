        <div class="list-group">
            <a href="/klienci" class="list-group-item {% if app.request.attributes.get( '_route' ) starts with 'klie' %} active {% endif %}">Klienci</a>
            <a href="/zlecenia" class="list-group-item {% if app.request.attributes.get( '_route' ) starts with 'zlece' %} active {% endif %}"">Zlecenia</a>
